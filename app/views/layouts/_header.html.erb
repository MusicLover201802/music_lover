<div class="row user_header navbar-fixed-top", style="background-color: #FFC107">
	<div class="logo-box">
		<%= link_to lovers_root_path do %>
			<img class="Logo" src="/ml_logo1.png" width="90" height="90">
		<% end %>
		<span class="nav-box">
			<div class="collapse navbar-collapse navbar-right" style="line-height: 90px" id="navbarEexample7">
		      　　<ul class="nav navbar-nav" style="margin-right: 80px">

		      	<% if user_signed_in? %>
		      		<li style="padding-right: 30px">こんにちは <%= current_user.last_name %> さん</li>
		            	<%= link_to lovers_user_path(current_user) do %>
	                <li class="navlink" aria-hidden="true" style="color: #9d9d9d"><img src="/user.png" width="25" height="25"> マイページ</li>
		            	<% end %>
		            	<%= link_to destroy_user_session_path, method: :delete do %>
	                <li class="navlink" aria-hidden="true" style="color: #9d9d9d"><img src="/sign_out.png" width="25" height="25"> ログアウト</li>
		            	<% end %>
		            	<%= link_to lovers_root_path do %>
	        		<img class="Cart" src="/cart.png" width="40" height="40">
		            	<% end %>
		        <% else %>
		        	<li style="padding-right: 30px">こんにちは ゲスト さん</li>
		        		<%= link_to new_user_session_path do %>
	                <li class="navlink" aria-hidden="true" style="color: #9d9d9d"><img src="/sign_in.png" width="25" height="25"> ログイン</li>
		            	<% end %>
		            	<%= link_to new_user_registration_path do %>
	                <li class="navlink" aria-hidden="true" style="color: #9d9d9d"><img src="/key.png" width="25" height="25"> 新規登録</li>
		            	<% end %>
		        <% end %>
	            </ul>
		    </div>
		</span>
	</div>
</div>